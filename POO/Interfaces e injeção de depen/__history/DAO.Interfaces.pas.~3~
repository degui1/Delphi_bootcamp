unit DAO.Interfaces;

interface

uses
  System.SysUtils;

type

  iDAO<T : class> = interface
    ['{5D959BED-B3E5-4F84-8AC8-B443F15C83BA}']
    procedure update;
    procedure insert;
    procedure delete;
    function This : T;
    procedure Display ( aValue : TProc<String>);

  end;

implementation

end.
